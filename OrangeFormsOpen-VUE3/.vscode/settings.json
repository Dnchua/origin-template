{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll.stylelint": "explicit"
  },
  "eslint.validate": ["javascript", "vue", "html", "typescript"],
  "css.validate": false,
  "scss.validate": false,
  "stylelint.validate": ["css", "postcss", "scss", "vue", "sass"]
}
